{
  "executionRoleArn": "arn:aws:iam::641016094908:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "bottles",
          "image": "<IMAGE1_NAME>",
          "essential": true,
          "entrypoint": ["/bottles"],
          "portMappings": [
              {
                  "hostPort": 8080,
                  "protocol": "tcp",
                  "containerPort": 8080
              }
          ]
      }
  ],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "CodeDeployDemo"
}